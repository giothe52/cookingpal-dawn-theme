<h1>asd</h1>

{% for block in section.blocks  %}
  <img src="{{block.settings.image | img_url: 'large'}}" />
  <h3>{{ section.settings.heading-title }}</h3>
{% endfor %}
{% schema %}
  {
    "name": "Image with content",
    "max_blocks": 3,
    "blocks" : [
      {
        "type" : "image",
        "name" : "Image block",
        "settings" : [
          {
            "type" : "image_picker",
            "label" : "image",
            "id" : "images"
          },
          {
            "type" : "text",
            "label" : "Heading 3",
            "id" : "heading-title"
          }
        ]
      }
    ],
    "presets" : [
      {
        "name" : "Section 2"
      }
    ]
  }
{% endschema %}

